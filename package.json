{
  "name": "base9-vscode",
  "displyName": "base9 Theme",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/base9-theme/base9-vscode.git"
  },
  "version": "0.0.8",
  "publisher": "base9-theme",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:base9-vscode.generate"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "base9-vscode.generate",
        "title": "Generate base9 theme"
      }
    ],
    "configuration": {
      "title": "Base9",
      "properties": {
        "base9.palette": {
          "type": "string",
          "default": "181818-d8d8d8-ab4642-dc9656-f7ca88-a1b56c-86c1b9-7cafc2-ba8baf",
          "markdownDescription": "9 colors sepearated by `-`. [Click](command:base9-vscode.generate) to generate",
          "pattern": "([0-9a-fA-F]{6}-){8}[0-9a-fA-F]{6}"
        }
      }
    },
    "themes": [
      {
        "label": "Base 9",
        "uiTheme": "vs-dark",
        "path": "./themes/base9.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "base9-builder-node": "0.1.2",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@types/color": "^3.0.2",
    "@types/glob": "^7.1.4",
    "@types/lodash": "^4.14.178",
    "@types/mocha": "^9.0.0",
    "@types/mustache": "^4.1.2",
    "@types/node": "^14.18.4",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4",
    "vsce": "^2.6.3"
  }
}
