  "tokenColors": [
    {
      "name": "Keyword",
      "scope": [
        "constant.language.undefined",
        "keyword",
        "storage.modifier",
        "variable.language.this",
        "variable.language.super",
        "storage.type.function.js",
        "storage.type.function.ts",
        "storage.type.class.js",
        "storage.type.class.ts",
        "storage.type.js",
        "storage.type.ts"
      ],
      "settings": {
        "foreground": "#E0699F"
      }
    },
    {
      "name": "Escape",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#F13F39"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator",
        "constant.character.escape",
        "punctuation.definition.logical",
        "punctuation.definition.quote",
        "punctuation.definition.list",
        "punctuation.definition.para",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#F13F39"
      }
    },
    {
      "name": "Type",
      "scope": [
        "entity.name.type",
        "entity.other.attribute-name.class",
        "meta.attribute.class entity.other.attribute-name",
        "punctuation.definition.group",
        "support.type.primitive",
        "storage.type"
      ],
      "settings": {
        "foreground": "#3DCED6"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "storage.type.annotation",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#F97F2D"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "markup.quote",
        "constant.other.reference"
      ],
      "settings": {
        "foreground": "#F1B845"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "meta.separator"
      ],
      "settings": {
        "foreground": "#B9AA8B"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#ECEC13"
      }
    },
    {
      "name": "literal",
      "scope": [
        "constant.numeric",
        "constant.language.null",
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#ECEC13"
      }
    },
    {
      "name": "variable",
      "scope": [
        "variable",
        "meta.import variable.other.readwrite.alias"
      ],
      "settings": {
        "foreground": "#F7E6BC"
      }
    },
    {
      "name": "Instance Variable",
      "scope": [
        "variable.other.property",
        "variable.other.normal.shell",
        "punctuation.definition.variable.shell",
        "punctuation.definition.character-class",
        "constant.other.character-class",
        "entity.other.attribute-name",
        "support.type.property-name",
        "meta.import variable"
      ],
      "settings": {
        "foreground": "#66D86A"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "variable.other.enummember",
        "variable.other.constant",
        "entity.other.attribute-name.id",
        "meta.attribute.id entity.other.attribute-name",
        "punctuation.definition.character-class"
      ],
      "settings": {
        "foreground": "#F7E6BC"
      }
    },
    {
      "name": "Normal",
      "scope": null,
      "settings": {
        "foreground": "#F7E6BC"
      }
    },
    {
      "name": "Highlight",
      "scope": [
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "foreground": "#3DCED6"
      }
    },
    {
      "name": "Primary",
      "scope": [
        "heading",
        "markup.bold"
      ],
      "settings": {
        "foreground": "#3DCED6"
      }
    },
    {
      "name": "Secondary",
      "scope": [
        "markup.inline.raw",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#66D86A"
      }
    },
    {
      "name": "Link",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#66D86A"
      }
    },
    {
      "name": "Bold",
      "scope": [
        "heading"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    }
  ]
}