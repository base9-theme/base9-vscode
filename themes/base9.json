  "tokenColors": [
    {
      "name": "Keyword",
      "scope": [
        "constant.language.undefined",
        "keyword",
        "storage.modifier",
        "variable.language.this",
        "variable.language.super",
        "storage.type.function.js",
        "storage.type.function.ts",
        "storage.type.class.js",
        "storage.type.class.ts",
        "storage.type.js",
        "storage.type.ts"
      ],
      "settings": {
        "foreground": "#BA8BAF"
      }
    },
    {
      "name": "Symbols",
      "scope": [
        "keyword.operator",
        "constant.character.escape",
        "punctuation.definition.logical",
        "punctuation.definition.quote",
        "punctuation.definition.list",
        "punctuation.definition.para",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#AB4642"
      }
    },
    {
      "name": "Class",
      "scope": [
        "entity.name.type",
        "entity.other.attribute-name.class",
        "meta.attribute.class entity.other.attribute-name",
        "punctuation.definition.group",
        "support.type.primitive",
        "storage.type"
      ],
      "settings": {
        "foreground": "#7CAFC2"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "storage.type.annotation",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#DC9656"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "markup.quote",
        "constant.other.reference"
      ],
      "settings": {
        "foreground": "#F7CA88"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "meta.separator"
      ],
      "settings": {
        "foreground": "#9D9E9D"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#A1B56C"
      }
    },
    {
      "name": "literal",
      "scope": [
        "constant.numeric",
        "constant.language.null",
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#A1B56C"
      }
    },
    {
      "name": "variable",
      "scope": [
        "variable",
        "meta.import variable.other.readwrite.alias"
      ],
      "settings": {
        "foreground": "#D8D8D8"
      }
    },
    {
      "name": "Instance Variable",
      "scope": [
        "variable.other.property",
        "variable.other.normal.shell",
        "punctuation.definition.variable.shell",
        "punctuation.definition.character-class",
        "constant.other.character-class",
        "entity.other.attribute-name",
        "support.type.property-name",
        "meta.import variable"
      ],
      "settings": {
        "foreground": "#86C1B9"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "variable.other.enummember",
        "variable.other.constant",
        "entity.other.attribute-name.id",
        "meta.attribute.id entity.other.attribute-name",
        "punctuation.definition.character-class"
      ],
      "settings": {
        "foreground": "#D8D8D8"
      }
    },
    {
      "name": "Normal",
      "scope": null,
      "settings": {
        "foreground": "#D8D8D8"
      }
    },
    {
      "name": "Highlight",
      "scope": [
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "foreground": "#86C1B9"
      }
    },
    {
      "name": "Primary",
      "scope": [
        "heading",
        "markup.bold"
      ],
      "settings": {
        "foreground": "#7CAFC2"
      }
    },
    {
      "name": "Secondary",
      "scope": [
        "markup.inline.raw",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#86C1B9"
      }
    },
    {
      "name": "Link",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#7CAFC2"
      }
    },
    {
      "name": "Bold",
      "scope": [
        "heading"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    }
  ]
}