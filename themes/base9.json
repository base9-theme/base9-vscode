  "tokenColors": [
    {
      "name": "Keyword",
      "scope": [
        "constant.language.undefined",
        "keyword",
        "storage.modifier",
        "variable.language.this",
        "variable.language.super",
        "storage.type.function.js",
        "storage.type.function.ts",
        "storage.type.class.js",
        "storage.type.class.ts",
        "storage.type.js",
        "storage.type.ts"
      ],
      "settings": {
        "foreground": "#da9bff"
      }
    },
    {
      "name": "Escape",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#81bcff"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator",
        "constant.character.escape",
        "punctuation.definition.logical",
        "punctuation.definition.quote",
        "punctuation.definition.list",
        "punctuation.definition.para",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#81bcff"
      }
    },
    {
      "name": "Type",
      "scope": [
        "entity.name.type",
        "entity.other.attribute-name.class",
        "meta.attribute.class entity.other.attribute-name",
        "punctuation.definition.group",
        "support.type.primitive",
        "storage.type"
      ],
      "settings": {
        "foreground": "#ff9cab"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "storage.type.annotation",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#1fcae3"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "markup.quote",
        "constant.other.reference"
      ],
      "settings": {
        "foreground": "#a2c503"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "meta.separator"
      ],
      "settings": {
        "foreground": "#a3a3a3"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#02cd9b"
      }
    },
    {
      "name": "literal",
      "scope": [
        "constant.numeric",
        "constant.language.null",
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#02cd9b"
      }
    },
    {
      "name": "variable",
      "scope": [
        "variable",
        "meta.import variable.other.readwrite.alias"
      ],
      "settings": {
        "foreground": "#dadada"
      }
    },
    {
      "name": "Instance Variable",
      "scope": [
        "variable.other.property",
        "variable.other.normal.shell",
        "punctuation.definition.variable.shell",
        "punctuation.definition.character-class",
        "constant.other.character-class",
        "entity.other.attribute-name",
        "support.type.property-name",
        "meta.import variable"
      ],
      "settings": {
        "foreground": "#ffa254"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "variable.other.enummember",
        "variable.other.constant",
        "entity.other.attribute-name.id",
        "meta.attribute.id entity.other.attribute-name",
        "punctuation.definition.character-class"
      ],
      "settings": {
        "foreground": "#dadada"
      }
    },
    {
      "name": "Normal",
      "scope": null,
      "settings": {
        "foreground": "#dadada"
      }
    },
    {
      "name": "Highlight",
      "scope": [
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "foreground": "#ff9cab"
      }
    },
    {
      "name": "Primary",
      "scope": [
        "heading",
        "markup.bold"
      ],
      "settings": {
        "foreground": "#ff9cab"
      }
    },
    {
      "name": "Secondary",
      "scope": [
        "markup.inline.raw",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#ffa254"
      }
    },
    {
      "name": "Link",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#ff9cab"
      }
    },
    {
      "name": "Bold",
      "scope": [
        "heading"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    }
  ]
}