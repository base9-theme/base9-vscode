  "tokenColors": [
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "storage.modifier",
        "variable.language.this",
        "variable.language.super",
        "storage.type.function.js",
        "storage.type.function.ts",
        "storage.type.class.js",
        "storage.type.class.ts",
        "storage.type.js",
        "storage.type.ts"
      ],
      "settings": {
        "foreground": "#D671BE"
      }
    },
    {
      "name": "Symbols",
      "scope": [
        "keyword.operator",
        "constant.character.escape",
        "punctuation.definition.logical",
        "punctuation.definition.quote",
        "punctuation.definition.list",
        "punctuation.definition.markdown",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#C32D27"
      }
    },
    {
      "name": "Class",
      "scope": [
        "entity.name.type",
        "entity.other.attribute-name.class",
        "meta.attribute.class entity.other.attribute-name",
        "punctuation.definition.group",
        "support.type.primitive",
        "storage.type"
      ],
      "settings": {
        "foreground": "#62BADA"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "storage.type.annotation",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#EA9648"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "markup.quote",
        "constant.other.reference"
      ],
      "settings": {
        "foreground": "#FACB84"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "meta.separator"
      ],
      "settings": {
        "foreground": "#B6B2B2"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#B0D350"
      }
    },
    {
      "name": "literal",
      "scope": [
        "constant.numeric",
        "constant.language.null",
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#B0D350"
      }
    },
    {
      "name": "variable",
      "scope": [
        "variable",
        "meta.import variable.other.readwrite.alias"
      ],
      "settings": {
        "foreground": "#E8E9E9"
      }
    },
    {
      "name": "Instance Variable",
      "scope": [
        "variable.other.property",
        "variable.other.normal.shell",
        "punctuation.definition.variable.shell",
        "punctuation.definition.character-class",
        "constant.other.character-class",
        "entity.other.attribute-name",
        "support.type.property-name",
        "meta.import variable"
      ],
      "settings": {
        "foreground": "#6DDACB"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "variable.other.enummember",
        "variable.other.constant",
        "entity.other.attribute-name.id",
        "meta.attribute.id entity.other.attribute-name",
        "punctuation.definition.character-class"
      ],
      "settings": {
        "foreground": "#B0D350"
      }
    },
    {
      "name": "Normal",
      "scope": null,
      "settings": {
        "foreground": "#E8E9E9"
      }
    },
    {
      "name": "Highlight",
      "scope": [
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "foreground": "#6DDACB"
      }
    },
    {
      "name": "Primary",
      "scope": [
        "heading",
        "markup.bold"
      ],
      "settings": {
        "foreground": "#62BADA"
      }
    },
    {
      "name": "Secondary",
      "scope": [
        "markup.inline.raw",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#6DDACB"
      }
    },
    {
      "name": "Link",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#62BADA"
      }
    },
    {
      "name": "Bold",
      "scope": [
        "heading"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    }
  ]
}