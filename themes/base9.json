  "tokenColors": [
    {
      "name": "Keyword",
      "scope": [
        "constant.language.undefined",
        "keyword",
        "storage.modifier",
        "variable.language.this",
        "variable.language.super",
        "storage.type.function.js",
        "storage.type.function.ts",
        "storage.type.class.js",
        "storage.type.class.ts",
        "storage.type.js",
        "storage.type.ts"
      ],
      "settings": {
        "foreground": "#B2BF75"
      }
    },
    {
      "name": "Escape",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#66CAA5"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator",
        "constant.character.escape",
        "punctuation.definition.logical",
        "punctuation.definition.quote",
        "punctuation.definition.list",
        "punctuation.definition.para",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#66CAA5"
      }
    },
    {
      "name": "Type",
      "scope": [
        "entity.name.type",
        "entity.other.attribute-name.class",
        "meta.attribute.class entity.other.attribute-name",
        "punctuation.definition.group",
        "support.type.primitive",
        "storage.type"
      ],
      "settings": {
        "foreground": "#EBAB78"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "storage.type.annotation",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#1FCAE3"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "markup.quote",
        "constant.other.reference"
      ],
      "settings": {
        "foreground": "#81BCFF"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "meta.separator"
      ],
      "settings": {
        "foreground": "#969696"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#DAA6E7"
      }
    },
    {
      "name": "literal",
      "scope": [
        "constant.numeric",
        "constant.language.null",
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#DAA6E7"
      }
    },
    {
      "name": "variable",
      "scope": [
        "variable",
        "meta.import variable.other.readwrite.alias"
      ],
      "settings": {
        "foreground": "#D0D0D0"
      }
    },
    {
      "name": "Instance Variable",
      "scope": [
        "variable.other.property",
        "variable.other.normal.shell",
        "punctuation.definition.variable.shell",
        "punctuation.definition.character-class",
        "constant.other.character-class",
        "entity.other.attribute-name",
        "support.type.property-name",
        "meta.import variable"
      ],
      "settings": {
        "foreground": "#FF9CAB"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "variable.other.enummember",
        "variable.other.constant",
        "entity.other.attribute-name.id",
        "meta.attribute.id entity.other.attribute-name",
        "punctuation.definition.character-class"
      ],
      "settings": {
        "foreground": "#D0D0D0"
      }
    },
    {
      "name": "Normal",
      "scope": null,
      "settings": {
        "foreground": "#D0D0D0"
      }
    },
    {
      "name": "Highlight",
      "scope": [
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "foreground": "#EBAB78"
      }
    },
    {
      "name": "Primary",
      "scope": [
        "heading",
        "markup.bold"
      ],
      "settings": {
        "foreground": "#EBAB78"
      }
    },
    {
      "name": "Secondary",
      "scope": [
        "markup.inline.raw",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#FF9CAB"
      }
    },
    {
      "name": "Link",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#FF9CAB"
      }
    },
    {
      "name": "Bold",
      "scope": [
        "heading"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    }
  ]
}