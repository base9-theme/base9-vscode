    {
      "name": "Generic selectors (CSS/SCSS/Less/Stylus)",
      "scope": [
        "meta.selector"
      ],
      "settings": {
        "foreground": "#62D6E8"
      }
    },
    {
      "name": "Language Built-ins",
      "scope": [
        "support"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#EBFF87"
